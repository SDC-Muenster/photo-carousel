{
  "name": "sdc-photo-carousel",
  "version": "1.0.0",
  "description": "build backend server and database that can handle millions of records",
  "author": "jshamoo",
  "license": "ISC",
  "repository": {
    "url": "https://github.com/SDC-Muenster/photo-carousel.git"
  },
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "node server-build/server.js",
    "prod:server": "webpack --config webpack.server.js -d",
    "prod:react": "webpack --config webpack.config.js -d",
    "prod": "npm-run-all --parallel prod:*",
    "seed": "node database/seed.js",
    "data": "node database/writeToCsv.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "dev:build-react": "webpack --config webpack.config.js -d -w",
    "dev:build-server": "NODE_ENV=development webpack --config webpack.server.js --mode=development -w",
    "dev:start": "nodemon server-build/server.js",
    "dev": "npm-run-all --parallel dev:*"
  },
  "dependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "axios": "^0.19.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "csv-writer": "^1.5.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "fast-csv": "^3.4.0",
    "install": "^0.13.0",
    "jquery": "^3.4.1",
    "mongodb": "^3.3.2",
    "mongoose": "^5.7.7",
    "morgan": "^1.9.1",
    "newrelic": "^6.1.0",
    "nodemon": "^1.19.3",
    "npm": "^6.12.0",
    "pg": "^7.12.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "sequelize": "^5.21.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.8.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-jest": "^24.9.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
