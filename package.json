    "eslint-plugin-react": "^7.16.0",
    "isomorphic-style-loader": "^5.1.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "style-loader": "^1.0.0"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/setupTests.js"
    ]
  }
}
